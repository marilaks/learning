#include<iostream>
#include<cstring>
using namespace std;

int main(){
	char app[10];
	char find;
	int size;
	
	//getting size from user
	cout<<"enter the number of elements"<<endl;
	cin>>size;
	
	//getting input from user
	cout<<"enter the "<<size<<" words";
	for(int i = 0;i<size;i++){
		cin>>app[i];
	}
	
	//getting the element to be find from user
	cout<<"enter the element to be found"<<endl;
	cin>>find;
	
	//to check if i have the char in my input
	if(strchr(app,find))
		cout<<"present "<<endl;
	else
		cout<<"not"<<endl;
}
